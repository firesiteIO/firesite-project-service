# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=pull_from_fibase-config
VITE_FIREBASE_DATABASE_URL=pull_from_fibase-config
VITE_FIREBASE_PROJECT_ID=pull_from_fibase-config
VITE_FIREBASE_STORAGE_BUCKET=pull_from_fibase-config
VITE_FIREBASE_MESSAGING_SENDER_ID=pull_from_fibase-config
VITE_FIREBASE_APP_ID=1:pull_from_fibase-config

# Service URLs
VITE_FUNCTIONS_URL=pull_from_fibase-config
VITE_MCP_MAX_URL=http://localhost:3002
VITE_CHAT_SERVICE_URL=http://localhost:5173

# AI Configuration
VITE_CLAUDE_MODEL=claude-3-7-sonnet-20240229
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Development Configuration
VITE_ENVIRONMENT=development
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_CONTEXT_INTEGRATION=true
VITE_ENABLE_DEBUG_MODE=true

# Feature Flags
VITE_ENABLE_KANBAN_AI=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_OFFLINE_MODE=true
